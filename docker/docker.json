{
    "key": "docker",
    "label": "Docker",
    "description": "Docker command-line environment",
    "logo": "https://www.docker.com/sites/default/files/legal/small_v.png",
    "image": {
        "name": "ndslabs/dind",
        "tags": [
            "latest"
        ]
    },
    "ports": [
        {
            "port": 3000,
            "protocol": "http"
        }
    ],
    "display": "stack",
    "access": "external",
	"volumeMounts": [
		{
			"mountPath" : "/var/lib/docker"
		}
	],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 512,
        "memDefault": 50
    },
    "readinessProbe" : {
        "type" : "http",
        "path" : "/",
        "port" : 3000,
        "initialDelay": 5,
        "timeout" : 600
    },
    "tags": [
        "7"
    ],
    "privileged": true
}
