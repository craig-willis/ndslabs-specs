{
    "key": "dind",
    "label": "Docker",
    "description": "Docker command-line environment",
    "logo": "https://www.docker.com/sites/default/files/legal/small_v.png",
    "image": {
        "name": "ndslabs/dind",
        "tags": [
            "latest"
        ]
    },
    "ports": [
        {
            "port": 3000,
            "protocol": "http"
        }
    ],
    "display": "stack",
    "access": "external",
	"volumeMounts" : [
		"mountPath" : "/var/lib/docker"
	],
    "resourceLimits": {
        "cpuMax": 500,
        "cpuDefault": 100,
        "memMax": 512,
        "memDefault": 50
    },
    "tags": [
        "7"
    ],
    "privileged": true
}
